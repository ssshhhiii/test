/* pages/search/search.wxss */
page{
  padding: 20rpx;
  background-color: #dedede;
}
.search{
  display: flex;
  height: 60rpx;
  justify-content: space-between;
}
input{
  background-color: #ffffff;
  width: 85%;
  height: 60rpx;
}
text{
  background-color: #ffffff;
  width: 12%;
  text-align: center;
}
navigator{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  background-color: #ffffff;
  border-bottom: 3rpx solid #cccccc;
  margin: 10rpx 0;
  padding: 10rpx;
}